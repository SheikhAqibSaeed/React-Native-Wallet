{
  "expo": {
  "name": "ExpenseXpert",
  "slug": "mobile",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/images/app-icon.png",
  "scheme": "mobile",
  "userInterfaceStyle": "automatic",
  "newArchEnabled": true,
  "ios": {
    "supportsTablet": true
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/app2-icon.png",
      "backgroundColor": "#ffffff"
    },
    "edgeToEdgeEnabled": true,
    "package": "com.aqibsaeed.mobile"
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/images/favicon.png"
  },
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/app3-icon.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    ]
  ],
  "updates": {
    "enabled": true,
    "fallbackToCacheTimeout": 0,
    "url": "https://u.expo.dev/60f02e2b-a866-4bf5-8d98-333daaa1c8b6"
  },
  "experiments": {
    "typedRoutes": true
  },
  "extra": {
    "router": {},
    "eas": {
      "projectId": "60f02e2b-a866-4bf5-8d98-333daaa1c8b6"
    }
  }
}
}
